{
  "expo": {
    "name": "Marcus - Revision made easy",
    "slug": "marcuslearning",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/marcus_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/marcus_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.yiannisk.marcuslearning",
      "usesAppleSignIn": true,
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yiannisk.marcuslearning"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "94a95cdd-212e-4285-9052-1a930b1ac590"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/94a95cdd-212e-4285-9052-1a930b1ac590"
    },
    "plugins": ["expo-apple-authentication", "expo-font", ["expo-image-picker", {"photosPermission": "Allow $(Marcus) to access your camera roll, we need this so we can access your notes to turn into revision material."}], ["expo-document-picker", {"iCloudContainerEnvironment": "Production"}], ["@react-native-voice/voice", { "microphonePermission": "Allow $(Marcus) to access the microphone, this is so the assistant can here you speak and respond to you.", "speechRecognitionPermission": "Allow $(Marcus) to recognize user speech, this is so the assistant can understand your queries."}]]
  }
}
